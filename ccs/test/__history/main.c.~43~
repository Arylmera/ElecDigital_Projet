#include <main.h>
#include <math.h>

int b_value;
int b_otp_value;

void main()
{
   setup_low_volt_detect(FALSE);
   b_value = 0;
   b_otp_value = 0;
    
   while(true)
   {
   
   if (b_value < 10) { b_otp_value = b_value;}
   else {
   math.floor(0);
   }
   
   output_b( b_otp_value );
   output_high(PIN_E1);
   delay_ms(500);
   output_low(PIN_E1);
   b_value++;
   
   }

}

