#include <main.h>

int b_value;
int b_output_value;

void main()
{
   setup_low_volt_detect(FALSE);
   b_value = 0;
    
   while(true)
   {
   
   b_output_value = b_value;
   int tmp = b_value % 16;
   b_output_value += 16*tmp;

   output_b(b_output_value);
   output_high(PIN_E0);
   delay_ms(500);
   output_low(PIN_E0);
   b_value++;
   
   }

}
